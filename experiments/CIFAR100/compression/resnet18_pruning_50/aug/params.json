{
    "model_name": "resnet18",
    "resume": "resnet18_@77.23.tar",

    "learning_rate": 0.1,
    "weight_decay": 1e-03,
    "schedule": [20, 40, 60, 80],
    "gamma": 0.2,
    "batch_size": 256,
    "num_epochs": 100,
    "num_workers": 4,
    "augmentation": 1,
    "cuda": 1,

    "dataset": "cifar100",

    "input_info": {
        "sample_size": [1, 3, 32, 32]
      },
    "compression": [
        {
            "algorithm": "filter_pruning",
            "pruning_init": 0.1,
            "params": {
            "schedule": "exponential",
                "pruning_target": 0.5,
                "pruning_steps": 15,
                "filter_importance": "geometric_median"
            }
        }
    ],
    "cutmix": 1
}